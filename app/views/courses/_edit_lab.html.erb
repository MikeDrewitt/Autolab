<%= form_for @course, url: "edit/labUpdate", method: :patch, builder: FormBuilderWithDateTimeInput do |l| %>
	<% if @course.errors.any? %>
		<div id="error_explanation">
			<h2><%= pluralize(@course.errors.count, "error") %>
	      prohibited this course from being saved:</h2>

			<ul>
				<% @course.errors.full_messages.each do |msg| %>
				<li><%= msg %></li>
				<% end %>
			</ul>
		</div>
	<% end %>
<div class="row">
		<div class="col s2 ">
			<%= l.submit 'Save', { :class=> "btn primary" } %>
		</div>	
</div>	

	<% end %>