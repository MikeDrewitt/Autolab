<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
		<% if provider.to_s.titleize == "Shibboleth" %>
			<b><%= button_to "Sign in with MYUB", user_omniauth_authorize_path(provider), {:class => "btn primary"} %></b><br />
		<% else %>
			<%= button_to "Sign in with #{provider.to_s.titleize}", user_omniauth_authorize_path(provider), {:class => "btn primary"} %><br />
		<% end %>
	<% end -%>
<% end %>

